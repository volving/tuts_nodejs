<div class="weatherWidget">
	{{#each partials.weather.locations}}
		<div class="locatoin">
			<h3>{{name}}</h3>
			<a href="{{forecastUrl}}">
				<img src="{{iconUrl}}" alt="{{weather}}">
				{{weather}}, {{temp}}
			</a>
		</div>
	{{/each}}
	<small>Source: <a href="http://www.wunderground.com" title="Weather Underground">Weather Underground</a></small>
</div>